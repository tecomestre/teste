CREATE TABLE veiculos (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, PLACA VARCHAR(7), ANO INTEGER, CLIENTE_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE clientes (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, FONE VARCHAR(10), RENDA FLOAT, NOME VARCHAR(50), CPF VARCHAR(11), TIPOCLIENTE_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE tipos_clientes (ID INTEGER GENERATED ALWAYS AS IDENTITY NOT NULL, DESCRICAO VARCHAR(20), PRIMARY KEY (ID))
ALTER TABLE veiculos ADD CONSTRAINT veiculosCLIENTE_ID FOREIGN KEY (CLIENTE_ID) REFERENCES clientes (ID)
ALTER TABLE clientes ADD CONSTRAINT clntesTPOCLIENTEID FOREIGN KEY (TIPOCLIENTE_ID) REFERENCES tipos_clientes (ID)
